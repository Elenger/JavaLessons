package com.javarush.task.task03.task0314;

/*
Таблица умножения
*/

public class Solution {
    public static void main(String[] args) {
        //напишите тут ваш код
        int[][] MyTable = new int[10][10];

        for(int a = 0; a<10; a++) {
            for (int b = 0; b < 10; b++) {

                MyTable[a][b] = (a + 1) * (b + 1);
                System.out.print(MyTable[a][b] + " ");
            }
            System.out.println();
        }

    }
}
